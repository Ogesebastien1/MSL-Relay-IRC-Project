#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/server/node_modules/mime/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/server/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/node_modules:/mnt/c/Users/masso/Documents/Epitech/node_modules:/mnt/c/Users/masso/Documents/node_modules:/mnt/c/Users/masso/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/server/node_modules/mime/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/server/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/node_modules:/mnt/c/Users/masso/Documents/Epitech/node_modules:/mnt/c/Users/masso/Documents/node_modules:/mnt/c/Users/masso/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/masso/Documents/Epitech/projets/T-JSF-600-STG_10/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
